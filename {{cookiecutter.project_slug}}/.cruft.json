{
  "template": "https://github.com/dornech/the-hatchlor-enhanced",
  "checkout": null,
  "context": {
    "cookiecutter": {
      "full_name": "{{ cookiecutter.full_name }}",
      "email": "{{ cookiecutter.email }}",
      "project_name": "{{ cookiecutter.project_name }}",
      "project_slug": "{{ cookiecutter.project_slug }}",
      "pkg_name": "{{ cookiecutter.pkg_name }}",
      "project_short_description": "{{ cookiecutter.project_short_description }}",
      "open_source_license": "{{ cookiecutter.open_source_license }}",
      "target_python_version": "{{ cookiecutter.target_python_version }}",
      "target_os_platform": "{{ cookiecutter.target_os_platform }}",
      "github_username": "{{ cookiecutter.github_username }}",
      "project_repo": "{{ cookiecutter.project_repo }}",
      "pypi_username": "{{ cookiecutter.pypi_username }}",
      "tooling_environment": "{{ cookiecutter.tooling_environment }}",
      "__tooling_packages": "{{ cookiecutter.__tooling_packages }}",
      "commitmsg_linter": "{{ cookiecutter.commitmsg_linter }}",
      "__tooling_commitmsg_linter": "{{ cookiecutter.__tooling_commitmsg_linter }}",
      "lock_file_support": {% if cookiecutter.lock_file_support %}true{% else %}false{% endif %},
      "initial_commit": {% if cookiecutter.initial_commit %}true{% else %}false{% endif %},
      "_template": "https://github.com/dornech/the-hatchlor-enhanced.git"
    }
  },
  "directory": null,
  "skip": [
    ".git"
  ]
}
